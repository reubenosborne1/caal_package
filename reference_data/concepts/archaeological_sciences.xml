    <skos:prefLabel xml:lang="en">{"id": "0222e651-fd26-408c-87a1-c5a882b2cdce", "value": "ARCHAEOMAGNETISM"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "6d875898-0e39-45c7-be10-0d13eaeec74a", "value": "Measures the remanent magnetisation direction of magnetic minerals. Useful for dating fired structures, in-situ since their last firing, and for sediments settling from non turbulent water bodies. In the UK, calibration data extends back to 1000BC."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "2992d172-d877-4575-b825-98e7d6c317eb", "value": "http://purl.org/heritagedata/schemes/560/concepts/142104"}</dcterms:identifier>
