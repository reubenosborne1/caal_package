    <skos:altLabel xml:lang="en">{"id": "34d9c128-b7e4-4e13-a09b-9b3fee3c6f20", "value": "Sem"}</skos:altLabel>
    <skos:prefLabel xml:lang="en">{"id": "8650418c-4a7b-4bbd-99f0-d9387d790c32", "value": "SCANNING ELECTRON MICROSCOPY"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "07c833cf-7601-439d-a01a-e30490a5995f", "value": "A process using an electron microscope in which the surface of the specimen is scanned by a beam of electrons which are reflected to form an image. Very high magnification is possible."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/182000e5-0e6d-307f-bdaf-d1584aea2fe6"/>
    <skos:altLabel xml:lang="en">{"id": "af40970b-3d3e-4583-9d6c-b75f2a1d14e2", "value": "S.E.M."}</skos:altLabel>
  </skos:Concept>
