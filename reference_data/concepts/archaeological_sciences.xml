    <skos:scopeNote xml:lang="en">{"id": "64016a11-c062-4c17-a534-13fdb9ce2968", "value": "The use of oxygen (O) isotope ratios in ice or ocean sediment cores to date global environmental change."}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "b5ef7ba8-1665-4bd6-a9b4-6a9ad0434de3", "value": "http://purl.org/heritagedata/schemes/560/concepts/142178"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/182000e5-0e6d-307f-bdaf-d1584aea2fe6"/>
