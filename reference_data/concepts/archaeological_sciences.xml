    <skos:altLabel xml:lang="en">{"id": "80e3d1db-899b-4874-ac8a-f5b1a2fbb70e", "value": "Carbon 14 Dating"}</skos:altLabel>
    <dcterms:identifier xml:lang="en-us">{"id": "5284dba5-61dc-4539-94e3-870d08dcf9ea", "value": "http://purl.org/heritagedata/schemes/560/concepts/142188"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en">{"id": "6ec64d1c-61f9-473e-8b5b-31922a778ec0", "value": "The measurement of the ratio of the radioactive Carbon 14 (C-14) isotope and non-radioactive carbon isotopes. Useful for dating organic materials such as wood and bone between 500 and 45,000 years old."}</skos:scopeNote>
    <skos:altLabel xml:lang="en">{"id": "5c24c27d-91fb-46cf-8dbb-ebea90361e90", "value": "Carbon Dating"}</skos:altLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1c931cfd-a931-3477-95ba-8fd822b78c7b">
    <skos:scopeNote xml:lang="en">{"id": "8c9983b6-2928-4226-84c4-98ab7f6de449", "value": "A relative dating technique for assessing bones from the same deposit. Often used to check for contemporaneity of bones selected for radiocarbon dating or to check for hoaxes such as the Piltdown Man."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/182000e5-0e6d-307f-bdaf-d1584aea2fe6"/>
