        <dcterms:identifier xml:lang="en-us">{"id": "ae87dd9a-7370-410e-a81e-bc1e0a62df7e", "value": "http://purl.org/heritagedata/schemes/560/concepts/142151"}</dcterms:identifier>
        <skos:inScheme rdf:resource="http://localhost:8000/182000e5-0e6d-307f-bdaf-d1584aea2fe6"/>
        <skos:scopeNote xml:lang="en">{"id": "86fd4db2-fb19-4496-abc9-573918e074b3", "value": "The light emitted from sedimentary minerals or mineral inclusions in bricks when stimulated in the laboratory by infrared light. Used to date samples up to 250,000 years old; especially appropriate for geological sediments containing feldspars"}</skos:scopeNote>
      </skos:Concept>
    </skos:narrower>
